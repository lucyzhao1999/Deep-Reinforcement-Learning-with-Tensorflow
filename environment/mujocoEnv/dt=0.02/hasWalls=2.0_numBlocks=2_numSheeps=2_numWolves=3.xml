<mujoco>
    <option gravity="0 0 0" timestep="0.02"/>
<default>
    <geom rgba="0 0 0 1"/>
</default>
    <worldbody>

    <light diffuse=".2 .2 .2" pos="0 0 20" dir="0 0 -1" mode="track"/>
    <body>
      <geom name='floor' pos='0 0 -0.4' size='10 10 .1' mass="10000" type='box' condim='3' rgba=".9 .9 .9 1" />
    <camera name="center" mode="fixed" pos="0 0 5" />
    <camera name="30" mode="fixed" pos="0 -5 10"  axisangle="1 0 0 30"    />
    </body>

    <body name="predator0" pos="0 0 0.075">
            <joint axis="1 0 0" damping="2.5" frictionloss="0" name="predator00" pos="0 0 0" type="slide"/>
            <joint axis="0 1 0" damping="2.5" frictionloss="0" name="predator01" pos="0 0 0" type="slide"/>
            <geom type="cylinder" size="0.075 0.075" mass="1" conaffinity="1" solimp="0.9 0.9999 0.001 0.5 1" solref="0.5 0.2" rgba="1 0 0 1"/>
            <site name="predator0" pos="-0.1 0 0.05" type="sphere" size="0.001"/>
    </body>

    <body name="predator1" pos="0 0 0.075">
            <joint axis="1 0 0" damping="2.5" frictionloss="0" name="predator10" pos="0 0 0" type="slide"/>
            <joint axis="0 1 0" damping="2.5" frictionloss="0" name="predator11" pos="0 0 0" type="slide"/>
            <geom type="cylinder" size="0.075 0.075" mass="1" conaffinity="1" solimp="0.9 0.9999 0.001 0.5 1" solref="0.5 0.2" rgba="1 0 0 1"/>
            <site name="predator1" pos="-0.1 0 0.05" type="sphere" size="0.001"/>
    </body>

    <body name="predator2" pos="0 0 0.075">
            <joint axis="1 0 0" damping="2.5" frictionloss="0" name="predator20" pos="0 0 0" type="slide"/>
            <joint axis="0 1 0" damping="2.5" frictionloss="0" name="predator21" pos="0 0 0" type="slide"/>
            <geom type="cylinder" size="0.075 0.075" mass="1" conaffinity="1" solimp="0.9 0.9999 0.001 0.5 1" solref="0.5 0.2" rgba="1 0 0 1"/>
            <site name="predator2" pos="-0.1 0 0.05" type="sphere" size="0.001"/>
    </body>
    <body name="prey0" pos="0 0 0.075">
            <joint axis="1 0 0" damping="2.5" frictionloss="0" name="prey00" pos="0 0 0" type="slide"/>
            <joint axis="0 1 0" damping="2.5" frictionloss="0" name="prey01" pos="0 0 0" type="slide"/>
            <geom type ="cylinder" size="0.05 0.075" mass="1" conaffinity="1" solimp="0.9 0.9999 0.001 0.5 1" solref="0.5 0.2" rgba="0 1 0 1"/>
            <site name="prey0" pos="0.5 0 0.075" type="sphere" size="0.001"/>
    </body>

    <body name="prey1" pos="0 0 0.075">
            <joint axis="1 0 0" damping="2.5" frictionloss="0" name="prey10" pos="0 0 0" type="slide"/>
            <joint axis="0 1 0" damping="2.5" frictionloss="0" name="prey11" pos="0 0 0" type="slide"/>
            <geom type ="cylinder" size="0.05 0.075" mass="1" conaffinity="1" solimp="0.9 0.9999 0.001 0.5 1" solref="0.5 0.2" rgba="0 1 0 1"/>
            <site name="prey1" pos="0.5 0 0.075" type="sphere" size="0.001"/>
    </body>


    <body name="block0" pos="0 0 0.075">
            <geom type="cylinder" size="0.2 0.2" mass="1" conaffinity="1" solimp="0.9 0.9999 0.001 0.5 1" solref="0.5 0.2" rgba="0.4 0.4 0.4 0.7" condim='3'/>

    </body>
    <body name="block1" pos="0 0 0.075">
        <geom type="cylinder" size="0.2 0.2" mass="1" conaffinity="1" solimp="0.9 0.9999 0.001 0.5 1" solref="0.5 0.2" rgba="0.4 0.4 0.4 0.7" condim='3'/>

    </body>

    <body>
      <geom name='wall1' pos='-2.25 0 -0.2' size='.25 2.5 0.5' mass="10000" type='box' rgba="0.4 0.4 0.4 0.3" condim='3'/>
    </body>
    <body>
      <geom name='wall2' pos='2.25 0 -0.2' size='.25 2.5 0.5' mass="10000" type='box' rgba="0.4 0.4 0.4 0.3" condim='3'/>
    </body>
    <body>
      <geom name='wall3' pos='0 -2.25 -0.2' size='2.5 .25 0.5' mass="10000" type='box' rgba="0.4 0.4 0.4 0.3" condim='3'/>
    </body>
    <body>
      <geom name='wall4' pos='0 2.25 -0.2' size='2.5 .25 0.5' mass="10000" type='box' rgba="0.4 0.4 0.4 0.3" condim='3'/>
    </body>

    </worldbody>

    <actuator>
        <motor gear="1" joint="predator00"/>
        <motor gear="1" joint="predator01"/>
        <motor gear="1" joint="predator10"/>
        <motor gear="1" joint="predator11"/>
        <motor gear="1" joint="predator20"/>
        <motor gear="1" joint="predator21"/>

        <motor gear="1" joint="prey00" />
        <motor gear="1" joint="prey01" />
        <motor gear="1" joint="prey10" />
        <motor gear="1" joint="prey11" />

    </actuator>
</mujoco>